{
  "name": "example-node-red-helper-test",
  "version": "1.0.0",
  "description": "This is the example of Node-red unit and integration tests, based on the module node-red-node-test-helper.",
  "scripts": {
    "test": "mocha --file ./test/**/*.spec.js",
    "test-unit": "mocha --file ./test/unit/*.spec.js",
    "test-integration": "mocha --file ./test/integration/*.spec.js",
    "start": "node $NODE_OPTIONS node_modules/node-red/red.js ./flows/flows.json -u ./"
  },
  "keywords": [
    "node-red",
    "test",
    "node-red-helper"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "node-red": "^1.3.5"
  },
  "devDependencies": {
    "node-red-node-test-helper": "github:jcbsfilho/node-red-node-test-helper",
    "mocha": "^9.0.0"
  }
}
